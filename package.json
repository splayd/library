{
  "name": "rumor-mill",
  "version": "0.0.0",
  "description": "A functional interface for interacting with databases",
  "keywords": [
    "database",
    "sql",
    "orm"
  ],
  "homepage": "https://github.com/splayd/rumor-mill",
  "bugs": "https://github.com/splayd/rumor-mill/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "splayd/rumor-mill",
  "scripts": {
    "test": "standard -v --fix && flow && ava",
    "release": "semantic-release",
    "prepack": "build-esm"
  },
  "dependencies": {
    "json-sql-builder2": "^1.0.19",
    "lodash": "^4.17.10",
    "mysql": "^2.15.0",
    "pg": "^7.4.3",
    "sqlite3": "^4.0.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-preset-diff": "^4.0.0",
    "babel-register": "^6.26.0",
    "build-esm": "^3.2.1",
    "dockerode": "^2.5.5",
    "flow-bin": "0.69.0",
    "semantic-release": "^15.6.3",
    "standard-esnext": "^4.0.3"
  },
  "babel": {
    "presets": [
      "diff"
    ]
  },
  "ava": {
    "concurrency": 4,
    "files": [
      "**/*.test.js",
      "**/test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
